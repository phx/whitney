[run]
source = core
omit =
    */tests/*
    */__init__.py
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass

[html]
directory = coverage_html
title = Fractal Field Theory Coverage Report

[xml]
output = coverage.xml 